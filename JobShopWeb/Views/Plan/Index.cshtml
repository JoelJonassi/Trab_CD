
<div class="p-4" style="border-radius:20px;">
		<div class="row">
			<div class="col-6">
				<h2 class="text-info">Production Plan</h2>

			</div>
			<div class="col-6 text-right">
				<a asp-action="Upsert" asp-controller = "Plan" class="btn btn-info text-dark"><i class="fas fa-plus"></i> &nbsp; Insert Data in Table</a>
				<a asp-action="ExportToExcellProductionPlan" asp-controller = "Plan" class="btn btn-info text-dark"><i class="fas fa-plus"></i> &nbsp; Export Table to Excell</a>

			</div>
		</div>
		<br />
		<br />
		<div style="background-color:#cde8e2" class="p-3 rounded">
		<table id="tblData" class="table table-striped table-bordered" style="width:100%">
			<thead>
				<tr>

					<th>Machine</th>
					<th>Job</th>
					<th>Op</th>
					<th>Initial Time</th>
					<th>Final Time</th>
					<th></th>
				</tr>

			</thead>
		</table>
		</div>
</div>
@section Scripts {
	<script src="~/js/plan.js"></script>
}